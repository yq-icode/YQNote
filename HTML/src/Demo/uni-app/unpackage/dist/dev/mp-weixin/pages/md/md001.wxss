page {
  background-color: #EFF0F1;
  background-image: url(data:image/jpeg;base64,);
  background-repeat: no-repeat;
  background-position: center top;
  background-size: 100% 180rpx;
}
scroll-view {
  position: absolute;
  left: 0;
  right: 0;
  top: 180rpx;
  bottom: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
}

